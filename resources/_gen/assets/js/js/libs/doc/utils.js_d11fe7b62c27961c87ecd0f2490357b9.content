var escapeChars={'¢':'cent','£':'pound','¥':'yen','€':'euro','©':'copy','®':'reg','<':'lt','>':'gt','"':'quot','&':'amp','\'':'#39'};var regex=new RegExp("[¢£¥€©®<>\"&']",'g');function escapeHTML(str){return str.replace(regex,function(m){return '&'+escapeChars[m]+';';});}
